@model MyProject.WebPresentation.Models.TrackingOrderViewModel

@{
    ViewBag.Title = "AddTrackHistory";
    Layout = "~/Areas/AdminPanel/Views/Shared/_adminLayout.cshtml";
}



<div class="header">
    <h1 class="page-title">Shipment Detail</h1>
</div>

<div class="main-content">
        @Html.HiddenFor(x => x.Order.OrderId)
        <div class="row">
            <div class="col-md-6">
                <div id="myTabContent" class="tab-content">
                    <div class="tab-pane active in" id="home">
                        <div id="tab">
                            <div class="form-group">
                                H. Airway Bill #:  @Html.DisplayFor(x => x.Order.AirWayBillNumberNumber)
                            </div>

                            <div class="form-group">
                                Shipper:  @Html.DisplayFor(x => x.Order.Shipper_SenderNameAddress)
                            </div>


                            <div class="form-group">
                                Consignee:  @Html.DisplayFor(x => x.Order.Consignee_ReceiverNameAddress)
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</div>



